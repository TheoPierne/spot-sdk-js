{
  "source": {
    "include": ["./bosdyn-client", "./bosdyn-core"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true
  }
}
